{
    "name": "@balancer/sdk",
    "description": "SDK for interacting with the Balancer protocol",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/balancer/b-sdk"
    },
    "publishConfig": {
        "access": "public"
    },
    "version": "5.1.0",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "codegen": "graphql-codegen --config codegen.yml",
        "prebuild": "pnpm run codegen",
        "build": "tsup",
        "format": "biome format --write .",
        "lint": "dpdm -T ./src && biome check .",
        "check-types": "tsc --noEmit --skipLibCheck",
        "test": "vitest dev",
        "test:all": "vitest run",
        "test:no-integration": "vitest run --exclude \"**/*.integration.test.ts\"",
        "test:no-v2": "vitest run --exclude \"test/v2/**\"",
        "changeset": "changeset",
        "changeset:release": "pnpm build && changeset publish --provenance",
        "example": "npx tsx ./examples/lib/executeExample.ts",
        "update:deployments": "npx tsx ./scripts/updateDeployments.ts && pnpm format"
    },
    "dependencies": {
        "@balancer-labs/balancer-maths": "0.0.38",
        "viem": "2.44.4"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@changesets/cli": "2.29.1",
        "@graphql-codegen/cli": "^6.0.0",
        "@graphql-codegen/typescript": "^5.0.0",
        "@graphql-codegen/typescript-operations": "^5.0.0",
        "@types/async-retry": "1.4.9",
        "@types/node": "22.14.1",
        "@viem/anvil": "0.0.10",
        "dotenv": "16.5.0",
        "dpdm": "3.14.0",
        "graphql": "^16.11.0",
        "graphql-tag": "^2.12.6",
        "pino-pretty": "13.0.0",
        "ts-node": "10.9.2",
        "tsup": "8.4.0",
        "typescript": "5.8.3",
        "vite": "6.4.1",
        "vite-tsconfig-paths": "5.1.4",
        "vitest": "3.1.1"
    },
    "packageManager": "pnpm@10.8.1",
    "engines": {
        "node": ">=18.x"
    }
}